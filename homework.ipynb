    "def search_array(arr, n):\n",
    "    for i in range(len(arr)):\n",
    "        if arr[i] == n:\n",
    "            return True\n",
    "    return False\n",
    "\n",
    "def main():\n",
    "    arr = [1, 2, 3, 4, 5]\n",
    "    n = 1\n",
    "    print(search_array(arr, n))\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    main()"
